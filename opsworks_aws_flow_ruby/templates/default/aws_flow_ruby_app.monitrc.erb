check process ruby_flow_runner-<%= @application_name %> with pidfile "/var/run/<%= @application_name %>.pid"
  start program = "<%= @deploy[:deploy_to] %>/current/runner-<%= @application_name %>.initrc start"
  stop program  = "<%= @deploy[:deploy_to] %>/current/runner-<%= @application_name %>.initrc stop"
  if 5 restarts within 5 cycles then timeout
